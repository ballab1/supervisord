#!/bin/bash


declare -ar env_www=(
    'WWW="${WWW:-/var/www}"'
    'WWW_UID="${WWW_UID:-$(www.UID)}"'
    'WWW_GID="${WWW_GID:-$(www.GID)}"'
    'WORKING_DIR="${WWW}"'
)

#remove WORKING_DIR so it comes after WWW
crf.removeFromRuntimeEnvironment 'WORKING_DIR' 
crf.updateRuntimeEnvironment "${env_www[*]}" 

crf.copyFileToRuntime 'LIB' 'www.bashlib'
